@{
    ViewData["Title"] = "Home Page";
}

@section scripts{
    <script src="https://cdn.jsdelivr.net/npm/vue/dist/vue.js"></script>
    <script>
        var app = new Vue({
            el: '#app',
            data: {
                message: 'Hello Vue!'
            }
        });
    </script>
}

<div class="container">
    <div id="app">
        <h5 v-bind:text="message"></h5>
    </div>
</div>


